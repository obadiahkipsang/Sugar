<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.chemisoft.sugar.MainActivity"
    tools:showIn="@layout/app_bar_main">


    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="350dp"
        android:layout_height="99dp"
        android:background="@color/bg_screen5"
        app:srcCompat="@drawable/emergency"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="16dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp" />

    <Button
        android:id="@+id/buttonapoint"
        android:layout_width="100dp"
        android:layout_height="80dp"
        android:background="@drawable/log"
        android:layout_below="@+id/imageButton"
        android:layout_alignLeft="@+id/imageButton"
        android:layout_alignStart="@+id/imageButton"
        android:layout_marginTop="32dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="100dp"
        android:layout_height="80dp"
        android:layout_alignTop="@+id/buttonapoint"
        android:layout_centerHorizontal="true"
        android:background="@drawable/bgl"
        android:layout_alignBottom="@+id/buttonapoint" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/buttonapoint"
        android:layout_alignStart="@+id/buttonapoint"
        android:layout_centerVertical="true"
        android:text="MyAppoiintments" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignLeft="@+id/button9"
        android:layout_alignStart="@+id/button9"
        android:text="Sugar Level" />

    <Button
        android:id="@+id/button10"
        android:layout_width="100dp"
        android:layout_height="80dp"
        android:layout_alignEnd="@+id/imageButton"
        android:layout_alignRight="@+id/imageButton"
        android:layout_alignTop="@+id/button9"
        android:background="@drawable/prescription"
        android:layout_alignBottom="@+id/button9" />

    <Button
        android:id="@+id/button11"
        android:layout_width="100dp"
        android:layout_height="80dp"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="32dp"
        android:background="@drawable/docs"
        />

    <Button
        android:id="@+id/button12"
        android:layout_width="100dp"
        android:layout_height="80dp"
        android:layout_alignBottom="@+id/button11"
        android:layout_alignLeft="@+id/textView6"
        android:background="@drawable/progress"
        android:layout_alignStart="@+id/textView6" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Doctor"
        android:layout_below="@+id/button11"
        android:layout_alignLeft="@+id/button11"
        android:layout_alignStart="@+id/button11"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button12"
        android:layout_alignStart="@+id/button12"
        android:layout_alignTop="@+id/textView7"
        android:text="MyProgress" />

    <Button
        android:id="@+id/button13"
        android:layout_width="100dp"
        android:layout_height="80dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/button12"
        android:background="@drawable/query"/>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Query"
        android:layout_alignBaseline="@+id/textView8"
        android:layout_alignBottom="@+id/textView8"
        android:layout_alignRight="@+id/button10"
        android:layout_alignEnd="@+id/button10"
        android:layout_marginRight="22dp"
        android:layout_marginEnd="22dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Prescriptions"
        android:layout_alignBaseline="@+id/textView6"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignLeft="@+id/button13"
        android:layout_alignStart="@+id/button13" />

    <Button
        android:id="@+id/button14"
        android:layout_width="100dp"
        android:layout_height="80dp"
        android:layout_marginTop="11dp"
        android:background="@drawable/learning"
        android:layout_below="@+id/textView7"
        android:layout_alignLeft="@+id/button11"
        android:layout_alignStart="@+id/button11" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Learning"
        android:layout_alignBottom="@+id/button14"
        android:layout_alignRight="@+id/textView7"
        android:layout_alignEnd="@+id/textView7" />

    <Button
        android:id="@+id/button15"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_alignEnd="@+id/button12"
        android:layout_alignRight="@+id/button12"
        android:layout_alignTop="@+id/button14"
        android:background="@drawable/quiz"
        android:layout_alignBottom="@+id/button14" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FAQS"
        android:layout_above="@+id/textView11"
        android:layout_toLeftOf="@+id/button10"
        android:layout_toStartOf="@+id/button10" />


</RelativeLayout>
